{
  "name": "syncing-server-js",
  "version": "1.0.0",
  "engines": {
    "node": ">=15.0.0 <17.0.0"
  },
  "description": "Syncing Server JS",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": "git@github.com:standardnotes/syncing-server-js.git",
  "author": "Karol SÃ³jko <karolsojko@gmail.com>",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "clean": "rm -fr dist",
    "prebuild": "yarn clean",
    "build": "tsc --rootDir ./",
    "lint": "eslint . --ext .ts",
    "pretest": "yarn lint && yarn build",
    "test": "jest --coverage",
    "start": "node dist/bin/server.js",
    "start:local": "npx nodemon -L bin/server.ts",
    "worker": "node dist/bin/worker.js",
    "daily-backup": "node dist/bin/backup.js",
    "daily-backup-no-email": "node dist/bin/backup.js mute-emails"
  },
  "dependencies": {
    "@newrelic/native-metrics": "7.0.2",
    "@sentry/node": "^6.16.1",
    "@standardnotes/auth": "3.8.3",
    "@standardnotes/common": "1.3.0",
    "@standardnotes/domain-events": "2.10.0",
    "@standardnotes/domain-events-infra": "1.1.0",
    "@standardnotes/features": "1.8.0",
    "@standardnotes/settings": "1.4.0",
    "@standardnotes/time": "1.0.4",
    "aws-sdk": "2.880.0",
    "axios": "0.24.0",
    "cors": "2.8.5",
    "dayjs": "1.9.7",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "4.3.1",
    "inversify": "5.0.1",
    "inversify-express-utils": "6.3.2",
    "ioredis": "4.19.4",
    "jsonwebtoken": "8.5.1",
    "mysql2": "2.2.5",
    "newrelic": "8.6.0",
    "nodemon": "2.0.7",
    "prettyjson": "1.2.1",
    "reflect-metadata": "0.1.13",
    "sqs-consumer": "5.5.0",
    "ts-node": "9.1.1",
    "typeorm": "0.2.29",
    "ua-parser-js": "0.7.24",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@standardnotes/config": "2.0.1",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/inversify-express-utils": "^2.0.0",
    "@types/ioredis": "^4.17.10",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/newrelic": "^7.0.2",
    "@types/prettyjson": "^0.0.29",
    "@types/ua-parser-js": "^0.7.35",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "jest": {
    "preset": "./node_modules/@standardnotes/config/src/jest.json",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/Bootstrap/",
      "HealthCheckController",
      "/InMemory/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.ts"
    ]
  }
}
